{
  "selectedAuthType": "oauth-personal",
  "preferredEditor": "cursor",
  "mcpServers": {
    "tavilyExpert": {
      "url": "https://tavily.api.tadata.com/mcp/tavily/cohort-jasmine-dialect-r8wdqh"
    },
    "github": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-e",
        "GITHUB_PERSONAL_ACCESS_TOKEN",
        "ghcr.io/github/github-mcp-server"
      ]
    },
    "exa": {
      "command": "npx",
      "args": [
        "-y",
        "exa-mcp-server"
      ],
      "env": {
        "EXA_API_KEY": "$EXA_API_KEY"
      }
    },
    "brave-search": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-brave-search"
      ],
      "env": {
        "BRAVE_API_KEY": "$BRAVE_API_KEY"
      }
    },
    "firecrawl": {
      "command": "npx",
      "args": [
        "-y",
        "firecrawl-mcp"
      ],
      "env": {
        "FIRECRAWL_API_KEY": "$FIRECRAWL_API_KEY"
      }
    },
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/Users/abhimehrotra/.ssh"
      ]
    },
    "tavily": {
      "command": "npx",
      "args": [
        "-y",
        "tavily-mcp"
      ],
      "env": {
        "TAVILY_API_KEY": "$TAVILY_API_KEY"
      }
    },
    "kagimcp": {
      "command": "uvx",
      "args": [
        "kagimcp"
      ],
      "env": {
        "KAGI_API_KEY": "$KAGI_API_KEY",
        "KAGI_SUMMARIZER_ENGINE": "$KAGI_SUMMARIZER_ENGINE"
      }
    },
    "Notion": {
      "command": "npx",
      "args": [
        "-y",
        "@notionhq/notion-mcp-server"
      ],
      "env": {
        "OPENAPI_MCP_HEADERS": "{\"Authorization\": \"Bearer $NOTION_TOKEN\", \"Notion-Version\": \"2022-06-28\" }"
      }
    },
    "prisma-mcp": {
      "command": "npx",
      "args": ["-y", "prisma-mcp" ]
    },
    "mcp-server-puppeteer": {
      "command": "npx",
      "args": ["-y", "mcp-server-puppeteer" ]
    },
    "mcp-server-newsnow": {
      "command": "npx",
      "args": ["-y", "mcp-server-newsnow" ]
    },
    "mcp-server-context7": {
      "command": "npx",
      "args": ["-y", "mcp-server-context7" ]
    }
  }
}