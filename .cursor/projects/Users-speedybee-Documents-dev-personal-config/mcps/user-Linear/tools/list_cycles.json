{
  "name": "list_cycles",
  "description": "Retrieve cycles for a specific Linear team",
  "arguments": {
    "type": "object",
    "properties": {
      "teamId": {
        "type": "string",
        "description": "Team ID"
      },
      "type": {
        "description": "Filter: current, previous, next, or all",
        "type": "string",
        "enum": [
          "current",
          "previous",
          "next"
        ]
      }
    },
    "required": [
      "teamId"
    ],
    "$schema": "http://json-schema.org/draft-07/schema#"
  }
}