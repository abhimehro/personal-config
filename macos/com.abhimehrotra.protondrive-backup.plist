  <key>StandardOutPath</key>
  <string>/Users/abhimehrotra/Library/Logs/protondrive-backup.log</string>
  <key>StandardErrorPath</key>
  <string>/Users/abhimehrotra/Library/Logs/protondrive-backup.log</string>

