<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.controld.health</string>
    
    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/bin/ctrld-health.sh</string>
    </array>
    
    <key>StartInterval</key>
    <integer>60</integer>
    
    <key>RunAtLoad</key>
    <true/>
    
    <key>StandardOutPath</key>
    <string>/var/log/ctrld-health.log</string>
    
    <key>StandardErrorPath</key>
    <string>/var/log/ctrld-health.log</string>
    
    <key>UserName</key>
    <string>root</string>
</dict>
</plist>
